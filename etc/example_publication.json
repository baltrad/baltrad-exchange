{
"publication": {
   "_comment_":"All files matching filter should be injected in dex",
   "active":true,
   "connection":{
     "protocol":"dex",
     "address":"http://localhost:8080",
     "crypto":{
       "sign":true,
       "libname":"keyczar",
       "nodename":"anders-nzxt",
       "privatekey":"/opt/baltrad2/etc/bltnode-keys/anders-nzxt.priv"
     }
   },
   "filter":{
  	 "filter_type": "and_filter", 
  	 "value": [
  	   {"filter_type": "attribute_filter", 
  	    "name": "_bdb/source_name", 
        "operation": "in", 
        "value_type": "string", 
        "value": ["sehem","seang"]}, 
  	   {"filter_type": "attribute_filter", 
        "name": "/what/object", 
        "operation": "=", 
        "value_type": "string", 
        "value": ["SCAN"]}
    ]
  }
}
}
