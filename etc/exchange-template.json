{
    "template":{
        "_comment_":"Template to use when creating publications / subscriptions.",
        "name":"Publishes all swedish scans",
        "publication": {
            "class":"baltrad.exchange.net.publishers.standard_publisher",
            "extra_arguments": {
                "threads":2,
                "queue_size":50
            },
            "connection": {
                "sender":"baltrad.exchange.net.senders.rest_sender",
                "encryption":"crypto"
            }
        },
        "filter":{
           "filter_type": "and_filter", 
           "value": [
                {"filter_type": "attribute_filter", 
                "name": "_bdb/source_name", 
                "operation": "like", 
                "value_type": "string", 
                "value": "se%"}, 
                {"filter_type": "attribute_filter", 
                 "name": "/what/object", 
                 "operation": "=", 
                 "value_type": "string", 
                 "value": ["SCAN"]}
            ]
        }
    }
}
