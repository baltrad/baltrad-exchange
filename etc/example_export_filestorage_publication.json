{
"publication": {
   "_comment_":"Usually publications should use this publisher. Extra arguments is number of threads and queue_size.",
   "name":"Copy file to storage publication",
   "class":"baltrad.exchange.net.publishers.standard_publisher",
   "extra_arguments": {
  	 "threads":1,
  	 "queue_size":50
   },
   "active":false,
   "connection":{
     "_comment_":"Export files to a storage as a publisher.",
     "class":"baltrad.exchange.net.connections.simple_connection",
     "arguments":{
       "adaptor":{
	     "class":"baltrad.exchange.net.adaptors.storage_adaptor",
	     "arguments":{
	     	"file_storage":[
    	    	 "exporting_storage"
       	 	]
       	 }
       }
     }
   },
   "filter":{
  	 "filter_type": "and_filter", 
  	 "value": [
  	   {"filter_type": "attribute_filter", 
  	    "name": "_bdb/source_name", 
        "operation": "in", 
        "value_type": "string", 
        "value": ["sehem","seang"]}, 
  	   {"filter_type": "attribute_filter", 
        "name": "/what/object", 
        "operation": "=", 
        "value_type": "string", 
        "value": ["SCAN"]}
    ]
  },
  "decorators":[
    { "decorator":"rave_be_decorator.keep_quantities_decorator",
      "arguments":[
      	["DBZH", "VRADH", "TH"]
      ]
    }
  ]
}
}
